# simple X hotkey daemon config

# browser
super + b
	$BROWSER
# sound mixer
super + shift + m
	$TERMINAL -e pulsemixer
# music interface
super + v
	$TERMINAL -e ncmpcpp
# ytclip-play
super + y
	mpv $(xclip -o -selection c)
# Music
super + shift + p
	playlistload
super + c
	mpc toggle
super + n
	mpc next
super + shift + N
	mpc prev
super + {bracketleft, bracketright}
	mpc volume {-5, +5}
# screen shots
super + shift + s
	scrot -e 'mv $f ~/Pictures'
